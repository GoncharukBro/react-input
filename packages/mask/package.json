{
  "name": "@react-input/mask",
  "version": "1.2.7",
  "license": "MIT",
  "author": "Nikolay Goncharuk <goncharuk.bro@gmail.com>",
  "description": "React input component for masked input.",
  "keywords": [
    "react",
    "react-component",
    "react-hook",
    "react-mask",
    "react-input-mask",
    "input",
    "input-mask",
    "text-field",
    "mask",
    "masked",
    "format",
    "pattern",
    "replace"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoncharukBro/react-input.git",
    "directory": "packages/mask"
  },
  "bugs": {
    "url": "https://github.com/GoncharukBro/react-input/issues"
  },
  "homepage": "https://github.com/GoncharukBro/react-input/tree/main/packages/mask#readme",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "type": "module",
  "module": "dist/module/index.js",
  "main": "dist/node/index.cjs",
  "types": "dist/@types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module/index.js",
      "require": "./dist/node/index.cjs",
      "types": "./dist/@types/index.d.ts"
    },
    "./*": {
      "import": "./dist/module/*.js",
      "require": "./dist/node/*.cjs",
      "types": "./dist/@types/*.d.ts"
    },
    "./*.js": {
      "import": "./dist/module/*.js",
      "require": "./dist/node/*.cjs",
      "types": "./dist/@types/*.d.ts"
    },
    "./InputMask": {
      "import": "./dist/module/InputMask/index.js",
      "require": "./dist/node/InputMask/index.cjs",
      "types": "./dist/@types/InputMask/index.d.ts"
    },
    "./InputMask/*": {
      "import": "./dist/module/InputMask/*.js",
      "require": "./dist/node/InputMask/*.cjs",
      "types": "./dist/@types/InputMask/*.d.ts"
    },
    "./InputMask/*.js": {
      "import": "./dist/module/InputMask/*.js",
      "require": "./dist/node/InputMask/*.cjs",
      "types": "./dist/@types/InputMask/*.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "dist/@types/index.d.ts": [
        "dist/@types/index.d.ts"
      ],
      "*": [
        "dist/@types/*",
        "dist/@types/*/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "ts-node ../../scripts/build.ts",
    "release:major": "ts-node ../../scripts/release.ts major",
    "release:minor": "ts-node ../../scripts/release.ts minor",
    "release:patch": "ts-node ../../scripts/release.ts patch"
  },
  "dependencies": {
    "@react-input/core": "^1.0.14"
  },
  "peerDependencies": {
    "@types/react": ">=16.8",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  }
}
