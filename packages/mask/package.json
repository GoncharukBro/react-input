{
  "name": "@react-input/mask",
  "version": "1.0.23",
  "author": "Nikolay Goncharuk <goncharuk.bro@gmail.com>",
  "description": "React input component for masked input.",
  "keywords": [
    "react",
    "react-component",
    "react-hook",
    "react-mask",
    "react-input-mask",
    "input",
    "input-mask",
    "text-field",
    "mask",
    "masked",
    "format",
    "pattern",
    "replace"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoncharukBro/react-input.git",
    "directory": "packages/mask"
  },
  "bugs": {
    "url": "https://github.com/GoncharukBro/react-input/issues"
  },
  "homepage": "https://github.com/GoncharukBro/react-input/tree/main/packages/mask#readme",
  "license": "MIT",
  "module": "dist/module/index.js",
  "main": "dist/node/index.cjs",
  "types": "dist/@types/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module/index.js",
      "require": "./dist/node/index.cjs",
      "types": "./dist/@types/index.d.ts"
    },
    "./*": {
      "import": [
        "./dist/module/*.js",
        "./dist/module/*"
      ],
      "require": [
        "./dist/node/*.cjs",
        "./dist/node/*"
      ],
      "types": [
        "./dist/@types/*.d.ts",
        "./dist/@types/*"
      ]
    },
    "./*.js": {
      "import": [
        "./dist/module/*.js",
        "./dist/module/*"
      ],
      "require": [
        "./dist/node/*.cjs",
        "./dist/node/*"
      ],
      "types": [
        "./dist/@types/*.d.ts",
        "./dist/@types/*"
      ]
    }
  },
  "typesVersions": {
    "*": {
      "dist/@types/index.d.ts": [
        "dist/@types/index.d.ts"
      ],
      "*": [
        "dist/@types/*",
        "dist/@types/*/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "release:major": "ts-node ../../scripts/release.ts major",
    "release:minor": "ts-node ../../scripts/release.ts minor",
    "release:patch": "ts-node ../../scripts/release.ts patch",
    "remove:dist": "ts-node ../../scripts/remove-dist.ts",
    "clear:types": "ts-node ../../scripts/clear-types.ts",
    "restore:dist": "ts-node ../../scripts/restore-dist.ts",
    "build:rollup": "rollup --config rollup.config.js",
    "build": "npm run remove:dist && npm run build:rollup && npm run clear:types && npm run restore:dist && ts-node ../../scripts/console-success.ts"
  },
  "dependencies": {
    "@react-input/core": "^1.0.9"
  },
  "peerDependencies": {
    "@types/react": ">=16.8",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  }
}
